{
  "name": "vite-plugin-autobase",
  "private": false,
  "version": "0.0.3",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/autobase.umd.js",
  "module": "./dist/autobase.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/autobase.es.js",
    "require": "./dist/autobase.cjs.js"
  },
  "repository": "https://github.com/hankliu62/autobase-webpack-plugin.git",
  "author": "hank.liu",
  "license": "MIT",
  "scripts": {
    "build:types": "rimraf ./types && tsc -p dtsconfig.json",
    "prebuild": "npm run build:types",
    "build": "tsc && vite build",
    "eslint": "eslint --ext .ts,.js src",
    "eslint:fix": "eslint --fix --ext .ts,.js src",
    "test": "jest",
    "doc": "rimraf ./docs && typedoc src --out ./docs --hideGenerator --excludeExternals --excludePrivate --readme readme.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "rimraf": "^5.0.5"
  }
}
